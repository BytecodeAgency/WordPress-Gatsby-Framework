  "scripts": {
    "build": "ts-node --require ./setup.js ./node_modules/.bin/gatsby build",
    "build-local": "NODE_ENV=development ts-node --require ./setup.js ./node_modules/.bin/gatsby build",
    "serve": "ts-node --require ./setup.js ./node_modules/.bin/gatsby serve",
    "compile": "tsc --skipLibCheck",
    "run-compiled": "node ./lib/api-server/index",
    "run-with-node": "node ./node_modules/.bin/gatsby build",
    "dev": "NODE_ENV=development ts-node --require ./setup.js ./node_modules/.bin/gatsby develop",
    "start": "ts-node ./src/api-server/index",
    "start-dev": "NODE_ENV=development ts-node ./src/api-server/index",
    "build:storybook": "build-storybook",
    "test": "jest",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
